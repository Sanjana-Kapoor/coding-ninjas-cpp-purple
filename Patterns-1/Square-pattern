#include<iostream>
using namespace std;


int main(){
    int n;
    cin>>n;
    
    int i=1;
    while(i<=n){
        int k=1;
        while(k<=n){
            cout<<n;
            k++;
        }
        cout<<endl;
        i++;
    }
    return 0;

       /*  Read input as specified in the question.
	* Print output as specified in the question.
	*/
  
}
