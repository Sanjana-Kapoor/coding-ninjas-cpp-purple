/* Print the following pattern for the given number of rows.
Pattern for N = 5
E
DE
CDE
BCDE
ABCDE */

#include<iostream>
using namespace std;


int main(){
    int n;
    cin>>n;
    
    int i=1;
    while(i<=n){
        int j=1;
        char start = 'A'+ n - i;
        
        while(j<=i){
             char ch = start +j-1;
            cout<<ch;
            j++;
        }
        cout<<endl;
        i++;
    }
    return 0;

       /*  Read input as specified in the question.
	* Print output as specified in the question.
	*/
  
}
