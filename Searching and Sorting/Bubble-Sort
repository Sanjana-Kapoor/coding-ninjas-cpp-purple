void bubbleSort(int arr[] , int n ){
  int counter = 0;
  while (counter < n - 1) {
    for (int i = 0; i < n - counter - 1; i++) {
       int temp;
      int a = arr[i];
      int b = arr[i + 1];
      if (arr[i] > arr[i + 1]) {
            temp=arr[i];
            arr[i]=arr[i+1];
            arr[i+1]=temp;
        
      }
    }
    counter++;
  }
}
